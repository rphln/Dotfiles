ALLITERATED = [
    (adjective, noun)
    for adjective in ADJECTIVES
    for noun in NOUNS
    if adjective[0] == noun[0]
]


def main():
    adjective, noun = choice(ALLITERATED)
    index = randint(0, 9999)

    print(f"{adjective}-{noun}-{index:04d}")


if __name__ == "__main__":
    main()
