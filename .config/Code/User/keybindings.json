[
  {
    "key": "ctrl+e",
    "when": "terminalFocus",
    "command": "ctrl+e"
  },
  {
    "key": "ctrl+j",
    "when": "editorTextFocus",
    "command": "editor.action.joinLines"
  },
  {
    "key": "alt+shift+]",
    "when": "editorTextFocus",
    "command": "cursorMove",
    "args": {
      "to": "nextBlankLine",
      "by": "wrappedLine"
    }
  },
  {
    "key": "alt+shift+[",
    "when": "editorTextFocus",
    "command": "cursorMove",
    "args": {
      "to": "prevBlankLine",
      "by": "wrappedLine"
    }
  },
  {
    "key": "ctrl+k tab",
    "when": "editorTextFocus",
    "command": "type",
    "args": { "text": "\t" }
  }
]
